{
    "name": "MNav",
    "title": "MNav",
    "version": "1.0.1",
    "main": "dist/assets/js/mnav.js",
    "description": "Collapsible navigation that hides menu items if they don't fit in navigation container.",
    "homepage": "https://mekristaps.github.io/MNav/",
    "author": {
        "name": "Kristaps Melnis",
        "url": "https://instagram.com/mekristaps"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mekristaps/MNav.git"
    },
    "keywords": [
        "mnav",
        "more navigation",
        "dropdown",
        "priority plus",
        "priority+",
        "responsive",
        "navigation",
        "nav"
    ],
    "license": {
        "type": "MIT",
        "url": "https://github.com/mekristaps/MNav/blob/master/LICENSE"
    },
    "bugs": {
